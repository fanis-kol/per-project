<!DOCTYPE html>
<html>
<head>
  <title>
    <%= title %>
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
  <div class="navbar">
    <a href="http://localhost:3000/">Home</a>
    <a href="http://localhost:3000/student">Students</a>
    <a href="http://localhost:3000/trainer">Trainers</a>
    <a href="http://localhost:3000/assignment">Assignment</a>
    <a href="http://localhost:3000/course">Courses</a>
    <a href="http://localhost:3000/student/add">New Student</a>
    <a href="http://localhost:3000/trainer/add">New Trainer</a>
    </div>
<div>
<table style="width:100%" >
  <thead>
    <th>Id</th>
    <th>Name</th>
    <th>Last Name</th>
    <th>Date Of Birth</th>
    <th>Tuition Fees</th>
  </thead>
  <tbody>
  <tr>
  <% for(var i = 0; i < student.length; i++) { %>
  <tr><%= student[i].id %></tr>
  <td>
    <%= student[i].student_id %>
  </td>
  <td>
    <%= student[i].name %>
  </td>
  <td>
    <%= student[i].last_name %>
  </td>
  <td>
    <%= new Date(student[i].date_of_birth).toLocaleString() %>
  </td>
  <td>
    <%= student[i].tuition_fees %>
  </td>
 
  <% } %>
</tr>
  </tbody>
</table>
</div>
</body>
</html>